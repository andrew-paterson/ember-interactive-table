{
  "name": "ember-interactive-table",
  "version": "0.3.21",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "rollup --config",
    "lint": "concurrently 'pnpm:lint:*(!fix)' --names 'lint:'",
    "lint:fix": "concurrently 'pnpm:lint:*:fix' --names 'fix:'",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "rollup --config --watch",
    "test": "echo 'A v2 addon does not have tests, run tests in test-app'"
  },
  "dependencies": {
    "@babel/core": "^7.23.6",
    "@embroider/addon-shim": "^1.9.0",
    "@embroider/util": "^1.13.2",
    "decorator-transforms": "^2.3.0",
    "lodash.merge": "^4.6.2",
    "ember-test-selectors": "^7.0.0",
    "flat": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@babel/runtime": "^7.26.0",
    "@embroider/addon-dev": "^7.1.1",
    "@rollup/plugin-babel": "^6.0.4",
    "babel-plugin-ember-template-compilation": "^2.3.0",
    "concurrently": "^9.1.0",
    "ember-template-lint": "^6.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^12.3.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.4.2",
    "rollup": "^4.29.1",
    "rollup-plugin-copy": "^3.5.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember-addon": {
    "app-js": {
      "./components/blank-template.js": "./dist/_app_/components/blank-template.js",
      "./components/ember-interactive-table/data-table-container.js": "./dist/_app_/components/ember-interactive-table/data-table-container.js",
      "./components/ember-interactive-table/data-table-row.js": "./dist/_app_/components/ember-interactive-table/data-table-row.js",
      "./components/ember-interactive-table/data-table.js": "./dist/_app_/components/ember-interactive-table/data-table.js",
      "./components/ember-interactive-table/expand-collapse-row-button.js": "./dist/_app_/components/ember-interactive-table/expand-collapse-row-button.js",
      "./components/ember-interactive-table/pagination-controls.js": "./dist/_app_/components/ember-interactive-table/pagination-controls.js",
      "./components/svg/arrow-down.js": "./dist/_app_/components/svg/arrow-down.js",
      "./components/svg/arrow-up.js": "./dist/_app_/components/svg/arrow-up.js",
      "./helpers/ember-interactive-table/extract.js": "./dist/_app_/helpers/ember-interactive-table/extract.js",
      "./helpers/ember-interactive-table/odd-or-even.js": "./dist/_app_/helpers/ember-interactive-table/odd-or-even.js",
      "./services/ember-interactive-table.js": "./dist/_app_/services/ember-interactive-table.js"
    },
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2
  },
  "ember": {
    "edition": "octane"
  },
  "peerDependencies": {
    "ember-source": ">= 4.0.0"
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./addon-main.js": "./addon-main.cjs"
  },
  "files": [
    "addon-main.cjs",
    "dist"
  ]
}
